{
  "private": true,
  "name": "web-app",
  "version": "0.1.0",
  "description": "PWA(Progressive Web App).",
  "author": "",
  "license": "ISC",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "test": "jest --config=config/jest.config.js",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "lint-js": "eslint src/ --no-eslintrc --config=.eslintrc.cjs --ext=js,ts --fix --quiet --cache --cache-location=node_modules/.cache/.eslintcache --format=pretty",
    "lint-css": "stylelint src/**/*.{css,scss,sass} --fix --quiet --cache --cache-location node_modules/.cache/.stylelintcache",
    "docs": "jsdoc -P ./package.json -R ./README.md -c ./config/jsdoc.config.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "axios": "^0.27.2",
    "core-js": "^3.22.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@babel/plugin-proposal-decorators": "7.18.10",
    "@babel/plugin-transform-runtime": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-typescript": "7.18.6",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "@wang1212/eslint-config": "0.2.1",
