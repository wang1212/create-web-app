    "css-minimizer-webpack-plugin": "4.0.0",
    "cssnano": "5.1.13",
    "del": "6.1.1",
    "docdash": "1.2.0",
    "eslint": "8.23.0",
    "eslint-formatter-pretty": "4.1.0",
    "eslint-plugin-compat": "4.0.2",
    "fork-ts-checker-webpack-plugin": "6.5.2",
